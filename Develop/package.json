{
  "name": "googlebooks-app",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"npm run watch:server\" \"npm run dev:client\"",
    "setup": "npm run setup:server && npm run setup:client",
    "build": "npm run build:client",
    "render-build": "npm install && npm run build",
    "watch:server": "nodemon server/server.js",
    "dev:client": "react-scripts start --prefix client",
    "setup:server": "npm install --prefix server",
    "setup:client": "npm install --prefix client",
    "build:client": "npm run build --prefix client"
  },
  
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "apollo-server-express": "^3.13.0",
    "graphql": "^16.8.1"
  }
}
